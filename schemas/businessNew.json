{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/companySearch.schema.json",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1
      },
      "description": {
        "type": "string",
        "minLength": 1,
        "maxLength": 200
      },
      "website": {
        "type": "string",
        "minLength": 1
      }
    },
    "additionalProperties": false,
    "required": [
        "name"
    ]
  }