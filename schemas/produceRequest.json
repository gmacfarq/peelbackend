{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "produceId": {
      "type": "integer",
      "minimum": 1
    },
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "quantityInLbs": {
      "type": "integer",
      "minimum": 1
    },
    "price": {
      "type": "number",
      "minimum": 1
    },
    "requestDate": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": [
    "produceId",
    "username",
    "quantityInLbs",
    "price",
    "requestDate"
  ]
}